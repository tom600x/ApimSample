        {
            "label": "run-api",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/ApimSample.Api/ApimSample.Api.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "run-mvc",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/ApimSample.MvcClient/ApimSample.MvcClient.csproj"
